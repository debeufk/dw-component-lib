{
	"compilerOptions": {
		"baseUrl": ".",
		"declaration": true,
		"experimentalDecorators": true,
		"forceConsistentCasingInFileNames": true,
		"importHelpers": true,
		"jsx": "react",
		"module": "commonjs",
		"moduleResolution": "node",
		"noImplicitAny": true,
		"noUnusedLocals": true,
		"preserveConstEnums": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strictNullChecks": true,
		"target": "es5"
	},
	"include": ["src"]
}
